# Production Environment Variables for Vercel Deployment
# Copy these to your Vercel project settings

# Node Environment
NODE_ENV=production

# Superset Configuration
SUPERSET_URL=https://bi.sdxpartners.com
SUPERSET_ADMIN_TOKEN=your_production_superset_token_here

# Frontend Configuration
VITE_API_BASE_URL=https://portal.sdxpartners.com

# Server Configuration
PORT=3001

# CORS Configuration
CORS_ORIGIN=https://your-vercel-app-domain.vercel.app

# Security
SESSION_SECRET=your_secure_session_secret_here

# Database/Storage Configuration (for future use)
# DATABASE_URL=your_production_database_url_here
# REDIS_URL=your_redis_url_here

# Logging
LOG_LEVEL=info

# Feature Flags
ENABLE_DEBUG=false
ENABLE_ANALYTICS=true
